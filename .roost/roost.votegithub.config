app_deploy:
  type: text
  content: |
    #/bin/sh

    HELM_OPTS="--set clusterIP=${ROOST_CLUSTER_IP},registry=zbio,namespace=${ROOST_NAMESPACE}"

    helm upgrade votegithub-helm-vote helm-vote -n $ROOST_NAMESPACE --create-namespace --install $HELM_OPTS
